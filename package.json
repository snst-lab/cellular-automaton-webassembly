{
  "name": "cellular-automaton-webassembly",
  "version": "1.0.0",
  "description": "Cellular automaton which is a famous numerical experiment (Game of Life). I experimented that using Web Assembly with 'wasm-bindgen' and 'web-sys' crate.",
  "author": "Tanusuke",
  "license": "Apache-2.0",
  "main": "build/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snst-lab/cellular-automaton-webassembly.git"
  },
  "bugs": {
    "url": "https://github.com/snst-lab/cellular-automaton-webassembly/issues"
  },
  "homepage": "https://github.com/snst-lab/cellular-automaton-webassembly#readme",
  "postinstall": "node-prune",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.2",
    "browser-sync": "^2.26.3",
    "del": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^4.0.2",
    "path": "^0.12.7",
    "ts-loader": "^5.3.3",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.5.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "build": "webpack",
    "commit": "git add -A && git commit",
    "push:github": "git config user.name 'snst-lab' && git remote rm origin && git remote add origin git@github.com:snst-lab/cellular-automaton-webassembly.git && git push origin master",
    "pull:github": "git pull git@github.com:snst-lab/cellular-automaton-webassembly.git master --allow-unrelated-histories",
    "push:heroku": "heroku git:remote -a cellular-automaton-webassembly && git push heroku master",
    "pull:heroku": "git pull https://git.heroku.com/cellular-automaton-webassembly.git master --allow-unrelated-histories",
    "wasm": "bin/wasm-pack.exe build src/public/wasm --target web"
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "express": "^4.17.1",
    "redis": "^2.8.0"
  },
  "private": true
}
